{
  "name": "compyle-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:api": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json src/server/index.ts",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev\" --names \"API,WEB\" --prefix-colors \"blue,green\"",
    "seed": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json src/server/scripts/seed.ts",
    "setup": "bash setup.sh",
    "start": "npm run dev:all",
    "build": "next build",
    "lint": "eslint"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "bcrypt": "^6.0.0",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "framer-motion": "^12.23.26",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "mongoose": "^9.0.2",
    "next": "16.0.1",
    "pino-pretty": "^13.1.3",
    "react": "19.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "19.2.0",
    "react-hot-toast": "^2.6.0",
    "ts-node-dev": "^2.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
