{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "../../dist/server",
    "rootDir": ".",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": false,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
